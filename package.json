    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": "18.18.0",
    "yarn": "3.6.4"
  },
  "volta": {
    "node": "18.18.0",
